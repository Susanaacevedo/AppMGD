<p id="notice"><%= notice %></p>

<h1>Meta</h1>

<table>
  <thead>
    <tr>
      <th>Descripcion</th>
      <th>Fecinicio</th>
      <th>Fecfin</th>
      <th>Cantmeta</th>
      <th>Faltameta</th>
      <th>Estado</th>
      <th>Pagos</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @meta.each do |metum| %>
      <tr>
        <td><%= metum.descripcion %></td>
        <td><%= metum.fecinicio %></td>
        <td><%= metum.fecfin %></td>
        <td><%= metum.cantmeta %></td>
        <td><%= metum.faltameta %></td>
        <td><%= metum.estado %></td>
        <td><%= metum.pagos %></td>
        <td><%= link_to 'Show', metum %></td>
        <td><%= link_to 'Edit', edit_metum_path(metum) %></td>
        <td><%= link_to 'Destroy', metum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Metum', new_metum_path %>
